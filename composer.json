{
    "name":         "dexamped/kohana-resque",
    "description":  "Kohana wrapper for php-resque with minion support.",
    "license":      "BSD-3-Clause",
    "type":         "kohana-module",
    "keywords":     ["resque", "kohana"],
    "authors": [
        {
            "name": "Chris",
            "email": "dexamped@gmail.com",
            "role": "developer"
        }
    ],
    "bin": ["bin/resque"],
    "minimum-stability": "dev",
    "require": {
        "composer/installers":     "~1.0",
        "kohana/core":             ">=3.3",
        "php":                     ">=5.3.3",
        "chrisboulton/php-resque": "dev-master"
    },
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": ["type:kohana-module"]
        }
    }
}
